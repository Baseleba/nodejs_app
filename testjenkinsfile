pipeline{
    agent { docker{ image 'node:14'
                args '-u root:root'}}

    stages{
        stage('Dep install'){ 
            steps{
               // sh 'sudo chown -R 995:993 "/.npm"'
                sh 'npm install'
            }
        }
        stages ('package'){
            steps{
                sh 'npm pack'
            }
        }
    }
}